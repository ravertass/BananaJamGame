pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
c_flag_solid=0
c_flag_slope=1

c_transparent_clr=0

c_player_spr=064

c_max_dx=3
c_ddx=0.1

c_st_x=1
c_st_y=1

c_level_width=128
c_level_height=128

---- initialization ----

function _init()
 level=get_map(c_st_x,c_st_y,
  c_level_width,c_level_height)
 player=get_player(64,0,0)
end

function get_player(x,y,pl_no)
 return {
  x=x,y=y,
  dx=0,dy=0,
  max_dx=c_max_dx,
  ddx=c_ddx,
  spr=c_player_spr,
  pl_no=pl_no
 }
end

function 
get_map(st_x,st_y,width,height)
 return {
  solids=get_solids(st_x,st_y,
           width,height),
  slopes=get_slopes(st_x,st_y,
           width,height)
 }
end

function get_solids(
  st_x,st_y,width,height)
 return 
  get_flag_map(st_x,st_y,
   width,height,c_solid_flag)
end

function get_slopes(
  st_x,st_y,width,height)
 return 
  get_flag_map(st_x,st_y,
   width,height,c_slope_flag)
end

function get_flag_map(
  st_x,st_y,width,height,flag)
 flag_map={}
 
 for x=st_x,st_x+width do
  add(flag_map,{})
  for y=st_y,st_y+height do
   add(flag_map[x],
       has_flag(x,y,flag))
  end
 end
 
 return flag_map
end

-- checks if a pixel has a
-- flag and is not transparent
function has_flag(x,y,flag)
 return
  fget(
   mget(flr(x/8),
        flr(y/8)))==flag
  and
  pget~=c_transparent_clr
end

---- update ----

function _update()
 update_player(player)
end

function update_player(player)
 input(player)
 gravity(player)
 move(player)
end

function input(player)
 if btn(0,player.pl_no) then
  acc_left(player)
 elseif 
    btn(1,player.pl_no) then
  acc_right(player)
 else
  dec(player)
 end
end

function acc_left(player)
 player.dx=
  max(player.dx-player.ddx,
      -player.max_dx)
end

function acc_right(player)
 player.dx=
  min(player.dx+player.ddx,
      player.max_dx)
end

function dec(player)
 if player.dx<0 then
  player.dx+=player.ddx
 elseif player.dx>0 then
  player.dx-=player.ddx
 end
end

function gravity(player)

end

function move(player)
 player.x+=player.dx
end

---- draw ----

function _draw()
 cls()
 draw_map()
 draw_player(player)
end

function draw_map()
 map(0,0,0,0,16,16)
end

function draw_player(player)
 spr(player.spr,
     player.x,
     player.y)
end
